<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans https://www.springframework.org/schema/beans/spring-beans-4.3.xsd">

	<bean id="emp" class="com.ibm.Employee" autowire="byName">
		<property name="id" value="101" />
		<property name="name" value="Saikat" />
		<property name="salary" value="15000" />
	</bean>

	<bean id="parmanentAddress" class="com.ibm.Address">
		<property name="state" value="West Bengal" />
		<property name="city" value="Bankura" />
		<property name="country" value="India" />
	</bean>

	<bean id="currentAddress" class="com.ibm.Address">
		<property name="state" value="Karnataka" />
		<property name="city" value="Bengaluru" />
		<property name="country" value="India" />
	</bean>

</beans>
